{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "yarn install",
  "framework": "vite",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "x-vercel-protection-bypass",
          "value": "true"
        }
      ]
    }
  ]
}
